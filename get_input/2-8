import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    String[] goukei = new String[3];

    for ( int i = 0 ; i < 3 ; i++) {
        goukei[i] = sc.next();
    }

    String ans = "";
    for ( int i = goukei.length - 1 ; i >= 0  ; i-- ) {
        ans += goukei[i];
    }

    System.out.println(ans);

  }
}