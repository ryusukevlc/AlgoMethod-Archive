import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int num = sc.nextInt();
    int ans = 0;
    for (int i = 0 ; i < num ; i++) {
        if (i==0) {
            ans = sc.nextInt();
        } else {
            ans *= sc.nextInt();
        }
    }
    System.out.println(ans);

  }
}