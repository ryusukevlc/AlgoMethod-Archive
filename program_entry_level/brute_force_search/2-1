import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int num = sc.nextInt();
    int count = 0;
    for (int i = 1 ; i < (num + 1) ; i++ ) {
        if (i % 2 != 0) {
            if (i % 3 != 0) {
                if (i % 5 != 0) {
                    count++;
                }
            }
        }
    }
    System.out.println(count);
  }
}