import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int v = sc.nextInt();
    int index = -1;

    for (int i = 0 ; i < n ; i++) {
        int num = sc.nextInt();
        if (v == num) {
            index = i;
        }
    }
    System.out.println(index);

  }
}