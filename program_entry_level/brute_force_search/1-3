import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int count = 0;
    while (sc.hasNext()) {
      int num = sc.nextInt();
      if (0 < num) {
        count++;
      }
    }
    System.out.println(count);
  }
}