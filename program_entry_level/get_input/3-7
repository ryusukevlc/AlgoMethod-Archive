import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int ans = 0;
    for (int i = 0 ; i < n ; i++) {
        int kari = sc.nextInt();
        if (i == 0) {
            ans = kari;
            continue;
        } 
        if (ans > kari) {
            ans = kari;
        }
    }
    System.out.println(ans);
  }
}