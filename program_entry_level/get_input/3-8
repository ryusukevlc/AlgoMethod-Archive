import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    String goukei = "";
    for (int i = 0 ; i < n ; i++) {
        goukei += sc.next();
    }
    String[] array = goukei.split("");
    System.out.println(array.length);
  }
}