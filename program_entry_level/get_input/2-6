import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int ans = 0;
    for (int i = 0 ; i < 4 ; i++) {
        int kari = sc.nextInt();
        if (i == 0) {
            ans = kari;
        }

        if ( ans < kari) {
            ans = kari;
        }
    }
    System.out.println(ans);
  }
}