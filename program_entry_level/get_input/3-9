import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    String ans = "";
    for (int i=0 ; i < n ; i++) {
      String mozi = sc.next();
      String iti = mozi.substring(0, 1);
      ans += iti;
    }
    System.out.println(ans);

  }
}