import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int num = sc.nextInt();
    int[] array = new int[num];
    for (int i=0 ; i < num ; i++) {
        array[i] = sc.nextInt();
    }

    int ans = 0;
    for (int i=0 ; i < num ; i++) {
        ans += array[i];
    }

    System.out.println(ans);

  }
}