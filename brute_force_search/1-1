import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int ans = sc.nextInt();
    boolean isExisted = false;

    for (int i=0 ; i<n ; i++) {
        int num = sc.nextInt();
        if (ans == num) {
            isExisted = true;
            break;
        }
    }
    
    if (isExisted) {
        System.out.println("Yes");
    } else {
        System.out.println("No");
    }
  }
}