import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int v = sc.nextInt();
    int v_count = 0;
    for (int i = 0 ; i < n ; i++) {
        int num = sc.nextInt();
        if (num == v) {
            v_count++;
        }
    }

    System.out.println(v_count);

  }
}