import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    String s = sc.next();
    String c = sc.next();
    int len = s.length();
    boolean isExist = false;

    for (int i = 0 ; i < len ; i++) {
        if (s.substring(i, i+1).equals(c)) {
            isExist = true;
        }
    }

    if(isExist) {
        System.out.println("Yes");
    } else {
        System.out.println("No");
    }

  }
}