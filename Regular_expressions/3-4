import java.util.*;
public class Main {
  public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    String s = sc.next();
    if (s.matches("^\\w+\\@\\w+\\.\\w{1,4}$")) {
        System.out.println("Yes");
    } else {
        System.out.println("No");
    }
  }
}